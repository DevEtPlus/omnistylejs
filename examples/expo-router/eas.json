{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      }
    },

    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },

    "production": {
      "channel": "production",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {}
    }
  },

  "cli": {
    "promptToConfigurePushNotifications": false
  }
}
